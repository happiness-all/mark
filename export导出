export function exportBlob(res) {
  let blob = res.data;
  const strs = res.headers["content-disposition"];
  const list = strs.split(";");
  const fileName = decodeURIComponent(
    list[list.length - 1].split("=")[1]
  );
  const fileType = fileName.split(".");
  downloadBlob(blob, fileName);
}

function downloadBlob(blob, fileName) {
  if ("download" in document.createElement("a")) {
    // 非ie下载
    const elink = document.createElement("a");
    elink.download = fileName;
    elink.style.display = "none";
    elink.href = URL.createObjectURL(blob);
    document.body.appendChild(elink);
    elink.click();
    URL.revokeObjectURL(elink.href); // 释放URL对象
    document.body.removeChild(elink);
  } else {
    // ie10+下载
    navigator.msSaveOrOpenBlob(blob, fileName);
  }
} 
注意点：
如果response 的content-type 是response 的时候 请使用上面的方法
如果不是的话 请使用 let filename = res.headers['content-disposition'].split(';')[1];
            let name = filename.split('=')[1];
            let blob = new Blob([res.data]);
            let url = URL.createObjectURL(blob); 
            其他的一模一样
